<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net6.0;net7.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.10.0</PackageVersion>
        <Version>2022.10.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaabee;ZeroMQ;NetMQ;Message Bus;Abstractions</PackageTags>
        <Description>Abstraction for Zaabee.ZeroMQ</Description>
        <PackageProjectUrl>https://github.com/PicoHex/Zaabee.ZeroMQ</PackageProjectUrl>
        <LicenseExpression>MIT</LicenseExpression>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

</Project>
