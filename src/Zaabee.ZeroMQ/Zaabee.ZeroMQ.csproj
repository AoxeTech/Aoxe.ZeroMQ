<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2021.9.0</PackageVersion>
        <Version>2021.9.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaabee;ZeroMQ;NetMQ;Message Bus</PackageTags>
        <Description>Wraps and extensions for ZeroMQ.</Description>
        <PackageProjectUrl>https://github.com/Mutuduxf/Zaabee.ZeroMQ</PackageProjectUrl>
        <LicenseExpression>MIT</LicenseExpression>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="NetMQ" Version="4.0.1.6" />
        <PackageReference Include="Zaabee.ZeroMQ.Abstractions" Version="2021.4.0" />
        <PackageReference Include="Zaabee.Serializer.Abstractions" Version="2021.9.0" />
    </ItemGroup>

</Project>
