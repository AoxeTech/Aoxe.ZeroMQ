<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>2022.8.0.4</PackageVersion>
		<Version>2022.8.0.4</Version>
		<Authors>Mutuduxf</Authors>
		<Company>Mutuduxf</Company>
		<PackageTags>Zaabee;ZeroMQ;NetMQ;Message Bus</PackageTags>
		<Description>Wraps and extensions for ZeroMQ.</Description>
		<PackageProjectUrl>https://github.com/PicoHex/Zaabee.ZeroMQ</PackageProjectUrl>
		<LicenseExpression>MIT</LicenseExpression>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
		<Optimize>true</Optimize>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="NetMQ" Version="4.0.1.10" />
		<PackageReference Include="Zaabee.ZeroMQ.Abstractions" Version="2022.8.0" />
		<PackageReference Include="Zaabee.Serializer.Abstractions" Version="2022.14.0" />
	</ItemGroup>
</Project>